<div class="vintage__container">

  <% @articles.each do |n| %>
  <ul>
    * <%= n.newsletter.title %> -
    Edition: <%= n.newsletter.sub_title %> -
    <%= n.title %> -
    Author: <%= n.byline %> -
    <% if n.photo.attached? %>
      <%= image_tag url_for(n.photo), :class=> "edit-box" %>
    <% else %>
      <div>No Image</div>
    <% end %>
    <%= link_to "Review", article_path(n.id) %>

  </ul>

  <% end %>

  <li class="retro-font-last"><%= link_to "Create New", new_article_path %></li>
  </div>
